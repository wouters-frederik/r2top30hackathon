    "type": "library",
    "license": "beerware",

    "require": {
        "php": ">=5.4",
        "behat/behat": "~3.0",
        "behat/mink-extension": "~2.0"
    },

    "require-dev": {
        "behat/mink-goutte-driver": "~1.1",
        "behat/mink-selenium2-driver": "~1.2",
        "behatch/contexts": "*"
    },

    "autoload": {
        "psr-4": {
            "Behatch\\": "src/",
            "Sanpi\\Behatch\\": "src/class_aliases/"
        }
    },

    "config": {
        "bin-dir": "bin/"
    }
}
